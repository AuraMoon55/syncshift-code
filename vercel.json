{
    "installCommand": "yarn install-dep",
    "buildCommand": "yarn build",
    "outputDirectory": ".",
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"index.js"
        }
    ]
}
